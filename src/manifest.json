{
    "manifest_version": 2,
    "name": "Timezone",
    "description": "Timezone",
    "version": "0.0.18",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "management",
        "notifications",
        "contextMenus"
    ]
}